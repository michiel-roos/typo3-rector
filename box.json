{
    "datetime": "release_date",
    "finder": [
        {
            "notName": "/.*\\.md|.*\\.dist|Makefile/",
            "exclude": [
                "example",
                "examples",
                "test",
                "test_old",
                "Tests",
                "Tester",
                "vendor-bin"
            ],
            "in": [
                "bin",
                "config",
                "Migrations",
                "src",
                "utils",
                "vendor"
            ]
        }
    ],
    "compression": "GZ",
    "algorithm": "SHA512",
    "compactors": [
        "KevinGH\\Box\\Compactor\\Php",
        "KevinGH\\Box\\Compactor\\Json"
    ],
    "git-commit-short": "package_version",
    "git-version": "package_branch_alias_version",
    "main": "./bin/typo3rector",
    "output": "typo3rector"
}
